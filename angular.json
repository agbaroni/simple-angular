{
    "cli": {
        "analytics": false,
        "packageManager": "npm"
    },
    "projects": {
        "app": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "assets": [
                            "src/assets"
                        ],
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "outputPath": "dist",
                        "polyfills": [
                            "zone.js"
                        ],
                        "sourceMap": true,
                        "tsConfig": "tsconfig.app.json"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "app:build"
                    }
                }
            },
            "prefix": "app",
            "projectType": "application",
            "root": "",
            "sourceRoot": "src"
        }
    },
    "version": 1
}